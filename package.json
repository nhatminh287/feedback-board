{
  "name": "feedback-board",
  "version": "1.0.0",
  "description": "A simple feedback board application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "client-install": "cd client && npm install",
    "dev-full": "concurrently \"npm run dev\" \"npm run client\"",
    "build": "npm install && npm run client-install && npm run build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1"
  }
}
